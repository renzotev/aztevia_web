@media (min-width 480px)
	.google-maps
		height 220px
	
@media (min-width 768px)
	header
		.logo
			float left
		
		.navbar
			.navbar-collapse			
				.navbar-nav
					li
						width auto
						
						a
							text-align center
							padding 5px
							width 102px
							height 40px
							vertical-align middle
							line-height 13px
			
		.pink-line
			&.hidden
				display block !important
		
		.slogan
			padding 16px 0 15px 0
			margin-top 0
			float left

	.about-section
		.container
			.col-sm-10
				background-color rosado
				
	.google-maps
		height 330px
		
	.line-darkGray
		.col-xs-4
			.percent
				font-size 120px
				
			span
				font-size 36px
	
	.main-slider
		padding-top 159px
	
		.carousel
			.carousel-indicators
				bottom 120px

@media (min-width 992px)
	header
		.navbar
			.navbar-collapse			
				.navbar-nav
					li
						a
							width 134px
	
	.google-maps
		height 440px

@media (min-width 1200px)
	header
		.navbar
			.navbar-collapse			
				.navbar-nav
					li
						a
							width 162px